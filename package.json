    "postinstall": "opencollective postinstall"
  },
  "devDependencies": {
    "@types/geojson": "*",
    "@types/node": "*",
    "benchmark": "^2.1.4",
    "camelcase": "*",
    "d3-queue": "*",
    "decamelize": "*",
    "documentation": "*",
    "eslint": "*",
    "eslint-config-mourner": "*",
    "esm": "^3.0.55",
    "fs-extra": "*",
    "lerna": "2.8.0",
    "load-json-file": "*",
    "meow": "*",
    "progress": "*",
    "rollup": "*",
    "rollup-plugin-typescript": "*",
    "tape": "*",
    "typescript": "*",
    "write-json-file": "*",
    "yamljs": "*"
  },
  "dependencies": {
    "opencollective": "^1.0.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/turf",
    "logo": "https://opencollective.com/turf/logo.txt"
  }
