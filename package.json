{
  "name": "turf",
  "version": "7.0.0",
  "description": "A JavaScript library for performing geospatial operations with GeoJSON",
  "main": "turf",
  "module": "turf.mjs",
  "browser": "turf.min.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "turf.js",
    "turf.mjs",
    "turf.min.js"
